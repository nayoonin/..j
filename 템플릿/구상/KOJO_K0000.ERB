;=================================================
;   sub KOJO_ENCOUNTER_Kxxx
;=================================================
;   キャラ口上:遭遇時
;-------------------------------------------------
; Input:
;  ARGS					処理区分
;	[START]				調教開始時
;	[END]				調教終了時
;-------------------------------------------------
@KOJO_ENCOUNTER_K0000(ARGS)

SELECTCASE ARGS
	;-------------------------------------------------
	;【START】 調教開始時の口上
	;-------------------------------------------------
	CASE "START"

		;-------------------------------------------------
		;- １回のみの口上
		;-------------------------------------------------

		;初回調教
		IF !MARK:함락각인 && C_FIRSTTIME_遭遇(1)

			;購入で入手
			IF !CFLAG:イベント加入
				;PRINTFORMW 
			;イベントで入手
			ELSE
				;PRINTFORMW 
			ENDIF

		;성노예LV1
		ELSEIF !MARK:함락각인 && (ABL:성노예 == 1) && C_FIRSTTIME_遭遇(2)
			;PRINTFORMW 

		;성노예LV2
		ELSEIF !MARK:함락각인 && (ABL:성노예 == 2) && C_FIRSTTIME_遭遇(3)
			;PRINTFORMW 

		;隷属開始
		ELSEIF !MARK:함락각인 && MARK:예속각인 && C_FIRSTTIME_遭遇(4)
			;PRINTFORMW 

		;陥落開始
		ELSEIF !TALENT:상위함락 && MARK:함락각인 && C_FIRSTTIME_遭遇(5)
			;PRINTFORMW 

		;상위함락
		ELSEIF TALENT:상위함락 && C_FIRSTTIME_遭遇(6)
			;PRINTFORMW 

		;-------------------------------------------------
		;- ２回目以降
		;-------------------------------------------------
		ELSE

			;반발각인LV3
			IF MARK:반발각인 == 3
				;陥落開始
				IF MARK:함락각인
					;PRINTFORMW 
				;それ以外
				ELSE
					;PRINTFORMW 
				ENDIF
			;상위함락
			ELSEIF TALENT:상위함락
				;PRINTFORMW 
			;陥落開始
			ELSEIF MARK:함락각인
				;PRINTFORMW 
			;隷属開始
			ELSEIF MARK:예속각인
				;PRINTFORMW 
			;성노예LV2
			ELSEIF ABL:성노예 == 2
				;PRINTFORMW 
			;성노예LV1
			ELSEIF ABL:성노예 == 1
				;PRINTFORMW 
			;それ以外
			ELSE
				;PRINTFORMW 
			ENDIF

		ENDIF

;		;隷属で二人称変更
;		SIF MARK:예속각인
;			CSTR:二人称 = ご主人様

	;-------------------------------------------------
	;【END】 調教終了時の口上
	;-------------------------------------------------
	CASE "END"
		;반발각인LV3
		IF MARK:반발각인 == 3
			IF MARK:함락각인
				;PRINTFORMW 
			ELSE
				;PRINTFORMW 
			ENDIF
		;陥落開始
		ELSEIF MARK:함락각인
			;PRINTFORMW 
		;隷属開始
		ELSEIF MARK:예속각인
			;PRINTFORMW 
		;성노예LV2
		ELSEIF ABL:성노예 == 2
			;PRINTFORMW 
		;성노예LV1
		ELSEIF ABL:성노예 == 1
			;PRINTFORMW 
		;それ以外
		ELSE
			;PRINTFORMW 
		ENDIF
	;-------------------------------------------------
	;※ 該当無し
	;-------------------------------------------------
	CASEELSE
		RETURN
ENDSELECT
RETURN



