@TURN_EVENT_F(ARG:0,ARG:1,ARG:2)

;학원
SIF CFlag:(10001) == 1 && CFLAG:주민_모럴 < 100
	CALL TURN_EVENT_F_1

;지원주택
SIF CFlag:(10007) == 1
	CALL TURN_EVENT_F_7

;러브호텔
SIF CFlag:(10050) == 1
	CALL TURN_EVENT_F_50

;カジノ
SIF CFlag:(10053) == 1
	CALL TURN_EVENT_F_53

;훈련장
;훈련장配置かつ教官でない(教官のコスト徴収は生徒側で行う)
SIF CFLAG:훈련장関係者 == 1 && CFLAG:訓練_教官 == 0
	CALL TURN_EVENT_F_56






