;-------------------------------------------------
;ルリム・シャイコース
;都市防衛専用
;-------------------------------------------------
@ENEMY_4(L_STATUS , LS_STATUS)
#DIM REF L_STATUS
#DIMS REF LS_STATUS
#DIM TEMP

TEMP = (FLAG:都市_Lv / 50) + 5
LS_STATUS:CON_敵_NAME = 『Rlim・Sharikorth:Lv{TEMP}』

L_STATUS:CON_敵_EB = TEMP * 2 + 1
L_STATUS:CON_敵_HP = TEMP * TEMP * 6000
L_STATUS:CON_敵_ATK = TEMP * 3
L_STATUS:CON_敵_DEF = 5
L_STATUS:CON_敵_HIT = TEMP * TEMP + FLAG:都市_Lv
L_STATUS:CON_敵_SPD = 10
L_STATUS:CON_敵_주병기 = 6
L_STATUS:CON_敵_부병기 = 7
L_STATUS:CON_敵_弱点 = 3
L_STATUS:CON_敵_耐性 = 2
RETURN
