;-------------------------------------------------
;調査時のミニイベント抽選汎用関数
;ダンジョンに専用の定義関数がなかった時用
;-------------------------------------------------
@DUNGEON_MINIEV
#DIM MEV
SELECTCASE CHUSEN(33 , 33 , 33)
	CASE 0
		;財布発見
		MEV = 300
	CASE 1
		;金貨発見
		MEV = 301
	CASE 2
		;戦術教本発見
		MEV = 302
ENDSELECT
RETURN MEV


