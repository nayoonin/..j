;-------------------------------------------------
;アーティファクト
;-------------------------------------------------
@RPG_ARTIFACT(PAGE = 0)
#DIM       PAGE          ;현재参照中のページ
#DIM CONST PAGE_NUM = 30 ;ページ内のアーティファクトの数
#DIM       ART_EQUIP     ;装備対象スロット
#DIM       ART_ID        ;アーティファクトのID
#DIM       L_INPUT       ;RESULT分岐用
CLEARLINE LINECOUNT
DRAWLINE
CALL RPG_ARTIFACT_LIST(1 + PAGE * PAGE_NUM , 1 + (PAGE + 1) * PAGE_NUM)

DRAWLINE

IF PAGE <= 0
	PRINTFORM 　　　　　　
ELSE
	PRINTBUTTON "[100] 이전", 100
	PRINTFORM 　
ENDIF
IF PAGE >= 3
	PRINTFORM 　　　　　　
ELSE
	PRINTBUTTON "[150] 다음", 150
	PRINTFORM 　
ENDIF
PRINTFORM 　[300] 정리　
SIF Flag:遺物_連続消去 == 1
	SETCOLOR 100,250,250
PRINTFORM 　[400] 연속소거　
RESETCOLOR
PRINTFORM 　[500] 정렬　　
PRINTFORM 　[600] 비장비AF일괄소거　　
PRINTFORML [999] 돌아간다
INPUT
L_INPUT = RESULT
SELECTCASE L_INPUT
CASE 100
	PAGE = LIMIT(PAGE - 1, 0, 2)
CASE 150
	PAGE = LIMIT(PAGE + 1, 0, 2)
CASE 300
	CALL RPG_ARTIFACT_KILL_BLANK
CASE 400
	Flag:遺物_連続消去 = !Flag:遺物_連続消去
CASE 500
	CALL RPG_ARTIFACT_SORT
	PRINTFORMW 아티팩트를 다섯 항목의 합계치 순으로 정렬했습니다.
CASE 600
		CLEARLINE LINECOUNT
		PRINTFORML 현재 장비하고 있지 않은 아티팩트를 전부 소거하시겠습니까？
		PRINTFORML 
		PRINTFORM [100] 예　　
		PRINTFORML [999] 아니오
		INPUT
		L_INPUT = RESULT
		SELECTCASE L_INPUT
		CASE 100
			CALL RPG_ARTIFACT_ALL_DELETE
			PRINTFORML 
			PRINTFORMW 소거했습니다.
		CASE 999
			RESTART
		ENDSELECT
	
CASE 999
	Flag:遺物_連続消去 = 0
	CALL RPG_ARTIFACT_KILL_BLANK
	JUMP RPG_MAIN
CASEELSE
	ART_ID = L_INPUT
	SIF !INRANGE(ART_ID, 1, 90)
		RESTART
	SIF Flag:(15000 + ART_ID) == 0
		RESTART
	IF Flag:遺物_連続消去 == 1
		SIF Flag:(15300 + ART_ID) == 0
			CALL RPG_ARTIFACT_DELETE(ART_ID)
		RESTART
	ENDIF
	CLEARLINE LINECOUNT
	DRAWLINE
	CALL SHOW_ARTIFACT_DATA(ART_ID)
	DRAWLINE
	PRINTFORML 
	PRINTFORM [100] 장비　
	PRINTFORM [101] 강화(LP필요)　
	PRINTFORML [150] 소거　
	PRINTFORML [999] 돌아간다
	INPUT
	L_INPUT = RESULT
	SELECTCASE L_INPUT
	CASE 100
		IF Flag:(15300 + ART_ID) != 0
			ART_EQUIP = FINDELEMENT(FLAG, ART_ID, 15901, 15906)
			PRINTFORML 【%SaveStr:(5000 + ART_ID)%】%조사만처리(SaveStr:(5000 + ART_ID),"를")% 슬롯 {ART_EQUIP - 15900}에서 제외하시겠습니까？
			PRINTFORM [100] 예　　
			PRINTFORML [999] 아니오
			INPUT
			SELECTCASE RESULT
			CASE 100
				PRINTFORML 【%SaveStr:(5000 + ART_ID)%】의 장비를 해제했습니다.
				Flag:ART_EQUIP = 0
				Flag:(15300 + ART_ID) = 0
			CASE 999
			ENDSELECT
			RESTART
		ENDIF
		DO
			CLEARLINE LINECOUNT
			FOR ART_EQUIP, 1, 6
				IF Flag:(15900 + ART_EQUIP) == 0
					PRINTFORML [{ART_EQUIP}] 【슬롯 {ART_EQUIP}】＝미장비＝
				ELSE
					PRINTFORML [{ART_EQUIP}] 【슬롯 {ART_EQUIP}】【%SaveStr:(5000+(Flag:(15900 + ART_EQUIP)))%】%조사만처리(SaveStr:(5000+(Flag:(15900 + ART_EQUIP))),"가")% 장비되어 있습니다.
				ENDIF
			NEXT
			PRINTFORML [999] 돌아간다
			INPUT
			ART_EQUIP = RESULT
			SELECTCASE ART_EQUIP
			CASE 999
			CASEELSE
				SIF !INRANGE(ART_EQUIP, 1, 5)
					CONTINUE
				Flag:(15300 + (Flag:(15900 + ART_EQUIP))) = 0
				Flag:(15900 + ART_EQUIP) = ART_ID
				Flag:(15300 + ART_ID) = 1
				PRINTFORMW 슬롯 {ART_EQUIP}에 %조사처리(SaveStr:(5000 + ART_ID),"를")% 장비했습니다.
			ENDSELECT
			BREAK
		LOOP 1
	CASE 101
		CLEARLINE LINECOUNT
		PRINTFORML LP를 소비하여 강화하시겠습니까？
		PRINTFORML 
		PRINTFORM [100]  1회분 강화한다(LP10필요)　
		PRINTFORM [101]  5회분 강화한다(LP50필요)　
		PRINTFORML [999] 아니오
		INPUT
		L_INPUT = RESULT
		SELECTCASE L_INPUT
		CASE 100
			IF Flag:도시_현재LP <= 9
				PRINTFORML 
				PRINTFORMW LP가 부족합니다.
				RESTART
			ENDIF
			Flag:(15400 + ART_ID) += 1
			Flag:(15500 + ART_ID) += 1
			Flag:(15600 + ART_ID) += 1
			Flag:(15700 + ART_ID) += 1
			Flag:(15800 + ART_ID) += 1
			PRINTFORML 
			PRINTFORMW 강화했습니다.
			Flag:도시_현재LP -= 10
		CASE 101
			IF Flag:도시_현재LP <= 49
				PRINTFORML 
				PRINTFORMW LP가 부족합니다.
				RESTART
			ENDIF
			Flag:(15400 + ART_ID) += 5
			Flag:(15500 + ART_ID) += 5
			Flag:(15600 + ART_ID) += 5
			Flag:(15700 + ART_ID) += 5
			Flag:(15800 + ART_ID) += 5
			PRINTFORML 
			PRINTFORMW 강화했습니다.
			Flag:도시_현재LP -= 50
		CASE 999
			RESTART
		ENDSELECT
	CASE 150
		IF Flag:(15300 + ART_ID) != 0
			PRINTFORML 
			PRINTFORMW 장비중이므로, 소거할 수 없습니다.
			RESTART
		ENDIF
		CLEARLINE LINECOUNT
		PRINTFORML 정말로 소거하시겠습니까？
		PRINTFORML 
		PRINTFORM [100] 예　　
		PRINTFORML [999] 아니오
		INPUT
		L_INPUT = RESULT
		SELECTCASE L_INPUT
		CASE 100
			CALL RPG_ARTIFACT_DELETE(ART_ID)
			PRINTFORML 
			PRINTFORMW 소거했습니다.
		CASE 999
			RESTART
		ENDSELECT
	CASE 999
		RESTART
	ENDSELECT
	DRAWLINE
ENDSELECT
RESTART



