;=================================================
;   sub COM_FUNC_ABLE_Xxxx
;=================================================
;   CHK:コマンド実行条件
;-------------------------------------------------
; Output:
;  RESULT				真偽値
;-------------------------------------------------
@COM_FUNC_ABLE_X54

;Ｖ器具使用中は不可
SIF TEQUIP:Ｖ器具
	RETURN 0

;- PLAYERにペニスが無いと不可
SIF !VAR_PENIS(PLAYER)
	RETURN 0

;윤활が高くないとダメ
SIF PALAM:윤활 < PALAMLV:3
	RETURN 0

RETURN 1

