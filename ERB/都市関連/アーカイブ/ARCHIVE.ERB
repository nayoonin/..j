DRAWLINE
$AL_LOOP
IF FLAG:(17000+L_N) != 0
	PRINTFORML [{L_N}] %FLAGNAME:(17000+L_N)%
ELSE

ENDIF
L_N += 1
RESULT = 0
SIF L_N < 500
	GOTO AL_LOOP
DRAWLINE
PRINTFORML [999] - 돌아간다
